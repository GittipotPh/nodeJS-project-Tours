    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "parcel": "^2.12.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^3.2.5"
  },
  "targets": {
    "default": {
      "includeNodeModules": true
    }
  },
  "engines": {
    "node": ">=12"
  }
