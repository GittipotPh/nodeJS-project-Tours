{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express, and MongoDB",
  "main": "index.js",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "start:dev": "nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js ",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build": "parcel build ./public/js/index.js --target node --bundle-node-modules --out-dir ./public/js --out-file bundle.js",
    "starts": "parcel start ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
  },
  "author": "Gittipot",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@parcel/config-default": "^2.12.0",
    "@parcel/transformer-raw": "^2.12.0",
    "@stripe/stripe-js": "^3.3.0",
    "axios": "^1.6.8",
    "babel-loader": "^8.0.6",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.36.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mapbox-gl": "^3.2.0",
    "mongoose": "^5.13.22",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-mailjet": "^6.0.5",
    "nodemailer": "^6.9.13",
    "nodemailer-mailgun-transport": "^2.1.5",
    "pug": "^3.0.2",
